<?xml version="1.0" encoding="UTF-8"?>
<GuiXml >
    <Controls>
        <TopLevelControl name="RotationsQueueControl" layer="4">
            <OnUpdate>Rotations.OnUpdate(self, time)</OnUpdate>
            <Anchor point="CENTER" relativeTo="GuiRoot" relativePoint="CENTER" offsetY="150"/>
            <Controls>
				<Texture name="$(parent)Alert">
					<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" />
					<Dimensions x="75" y="75" />
				</Texture>
				<Texture name="$(parent)BarSwap" textureFile="/esoui/art/treeicons/gamepad/progression_levelup_weaponswap.dds" alpha="0">
					<Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER" />
					<Dimensions x="75" y="75" />
				</Texture>
				
				<Label name="$(parent)Key" font="ZoFontWinH1">
				  <Anchor point="CENTER" relativeTo="$(parent)" relativePoint="CENTER"/>
				</Label>
            </Controls>
        </TopLevelControl>
    </Controls>
</GuiXml>
